{
    "name": "generator-jhipster-react-native",
    "version": "0.0.0",
    "description": "React Native Blueprint for existing JHipster Apps",
    "keywords": [
        "yeoman-generator",
        "jhipster-blueprint",
        "react-native",
        "jhipster-7"
    ],
    "homepage": "https://github.com/ruddell/jhipster-react-native",
    "bugs": {
        "url": "https://github.com/ruddell/jhipster-react-native/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ruddell/jhipster-react-native.git"
    },
    "license": "Apache-2.0",
    "author": {
        "name": "Jon Ruddell",
        "email": "jonathan.ruddell@gmail.com",
        "url": "https://jruddell.com/"
    },
    "main": "generators/app/index.js",
    "files": [
        "generators",
        "lib"
    ],
    "scripts": {
        "jsdoc": "jsdoc --configure jsdoc-conf.json",
        "lint": "eslint .",
        "lint-fix": "npm run lint -- --fix",
        "prettier": "prettier --write \"{,**/}*.{js,json,md}\"",
        "release": "np",
        "precommit": "lint-staged",
        "pretest": "npm run lint",
        "test": "npm run test:unit -- test/*.spec.js",
        "test:unit": "mocha --timeout 120000 --slow 0 --reporter spec"
    },
    "lint-staged": {
        "{,generators/**/,lib/**/,cli/**/,test/**/}*.{js,json,md}": [
            "prettier --write",
            "eslint --fix --ext .js"
        ]
    },
    "dependencies": {
        "chalk": "4.1.0",
        "cross-spawn": "7.0.3",
        "fs-extra": "9.0.1",
        "generator-jhipster": "git://github.com/jhipster/generator-jhipster.git#819d07f1ca349ae6f8710f2a3387030a29964982",
        "lint-staged": "10.5.1",
        "pluralize": "8.0.0",
        "shelljs": "0.8.4"
    },
    "devDependencies": {
        "chai": "4.2.0",
        "ejs-lint": "1.1.0",
        "eslint": "7.13.0",
        "eslint-config-airbnb-base": "14.2.1",
        "eslint-config-prettier": "6.15.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-prettier": "3.1.4",
        "jsdoc": "3.6.6",
        "mocha": "8.2.1",
        "np": "7.0.0",
        "prettier": "2.1.2",
        "sinon": "9.2.1",
        "through2": "^4.0.2",
        "yeoman-assert": "3.1.1",
        "yeoman-test": "3.0.0"
    }
}
