{
    "name": "generator-jhipster-react-native",
    "version": "4.0.0-alpha.2",
    "description": "React Native Blueprint for existing JHipster Apps",
    "keywords": [],
    "homepage": "https://github.com/ruddell/jhipster-react-native",
    "bugs": {
        "url": "https://github.com/ruddell/jhipster-react-native/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ruddell/jhipster-react-native.git"
    },
    "license": "Apache-2.0",
    "author": {
        "name": "Jon Ruddell",
        "email": "jonathan.ruddell@gmail.com",
        "url": "https://jruddell.com/"
    },
    "main": "generators/app/index.js",
    "bin": {
        "rnhipster": "./cli/rnhipster.js"
    },
    "files": [
        "generators",
        "lib",
        "cli"
    ],
    "scripts": {
        "jsdoc": "jsdoc --configure jsdoc-conf.json",
        "lint": "eslint .",
        "lint-fix": "npm run lint -- --fix",
        "prettier": "prettier --write \"{,**/}*.{js,json,md}\"",
        "release": "np",
        "pretest": "npm run lint",
        "test": "npm run test:unit -- test/*.spec.js",
        "test:unit": "mocha --timeout 120000 --slow 0 --reporter spec"
    },
    "lint-staged": {
        "{,generators/**/,lib/**/,cli/**/,test/**/}*.{js,json,md}": [
            "prettier --write"
        ]
    },
    "dependencies": {
        "chalk": "4.1.0",
        "fs-extra": "9.0.1",
        "generator-jhipster": "7.0.0-beta.0",
        "pluralize": "8.0.0",
        "semver": "7.3.2",
        "lodash": "4.17.20",
        "shelljs": "0.8.4"
    },
    "devDependencies": {
        "chai": "4.2.0",
        "ejs-lint": "1.1.0",
        "eslint": "7.13.0",
        "eslint-config-airbnb-base": "14.2.1",
        "eslint-config-prettier": "6.15.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-prettier": "3.1.4",
        "jsdoc": "3.6.6",
        "lint-staged": "10.5.1",
        "mocha": "8.2.1",
        "np": "7.0.0",
        "prettier": "2.1.2",
        "sinon": "9.2.1",
        "yeoman-assert": "3.1.1",
        "yeoman-test": "3.0.0"
    },
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    }
}
