{
    "name": "generator-jhipster-react-native",
    "version": "0.0.0",
    "description": "React Native Blueprint for existing JHipster Apps",
    "keywords": [
        "yeoman-generator",
        "jhipster-blueprint"
    ],
    "homepage": "https://github.com/ruddell/generator-jhipster-react-native",
    "bugs": {
        "url": "https://github.com/ruddell/generator-jhipster-react-native/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ruddell/generator-jhipster-react-native.git"
    },
    "license": "Apache-2.0",
    "author": {
        "name": "Jon Ruddell",
        "email": "jonathan.ruddell@gmail.com",
        "url": "https://jruddell.com/"
    },
    "main": "generators/app/index.js",
    "files": [
        "generators"
    ],
    "scripts": {
        "jsdoc": "jsdoc --configure jsdoc-conf.json",
        "lint": "eslint .",
        "lint-fix": "npm run lint -- --fix",
        "prettier": "prettier --write \"{,**/}*.{js,json,md}\"",
        "release": "git push && git push --tags && npm publish",
        "release:major": "npm version major -a -m \"Update to %s\" && npm run release",
        "release:minor": "npm version minor -a -m \"Update to %s\" && npm run release",
        "release:patch": "npm version patch -a -m \"Update to %s\" && npm run release",
        "pretest": "npm run lint",
        "test": "npm run test:unit -- test/*.spec.js",
        "test:unit": "mocha --timeout 120000 --slow 0 --reporter spec"
    },
    "dependencies": {
        "chalk": "4.1.0",
        "cross-spawn": "^7.0.3",
        "fs-extra": "9.0.1",
        "fs-jetpack": "^4.0.0",
        "generator-jhipster": "git://github.com/jhipster/generator-jhipster.git#f0031a0e2d207b1af083e6f02daad103b401c60b",
        "lint-staged": "^10.4.2",
        "pluralize": "^8.0.0",
        "shelljs": "^0.8.4"
    },
    "devDependencies": {
        "chai": "4.2.0",
        "ejs-lint": "1.1.0",
        "eslint": "7.11.0",
        "eslint-config-airbnb-base": "14.2.0",
        "eslint-config-prettier": "6.14.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-prettier": "3.1.4",
        "jsdoc": "3.6.6",
        "mocha": "8.2.0",
        "prettier": "2.1.2",
        "sinon": "9.2.0",
        "yeoman-assert": "3.1.1",
        "yeoman-test": "3.0.0"
    }
}
