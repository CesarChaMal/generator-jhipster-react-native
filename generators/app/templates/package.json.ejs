{
  "name": "<%= context.reactNativeAppNameKebabCase %>",
  "version": "0.0.1",
  "scripts": {
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "test": "jest",
    "publish": "expo publish",
    "build:ios": "expo build:ios",
    "build:android": "expo build:android",
    "build:web": "expo build:web",
    <%_ if (!context.skipCommitHook) { _%>
    "precommit": "lint-staged",
    <%_ } _%>
    <%_ if (context.detox) { _%>
    "start:e2e": "expo start --no-dev --minify",
    "test:e2e": "./e2e/scripts/setup.sh && detox test --configuration ios.sim.release --retries 3",
    <%_ } _%>
    "postinstall": "patch-package",
    "prettier": "prettier --write \"{,.,**/,.jhipster/**/}*.{md,json,yml,js,ts,tsx}\""
  },
  "dependencies": {
    "@react-native-community/async-storage": "REPLACE_WITH_VERSION",
    "apisauce": "REPLACE_WITH_VERSION",
    "format-json": "REPLACE_WITH_VERSION",
    "identity-obj-proxy": "REPLACE_WITH_VERSION",
    "lodash": "REPLACE_WITH_VERSION",
    "querystringify": "REPLACE_WITH_VERSION",
    "ramda": "REPLACE_WITH_VERSION",
    "react-native-keyboard-aware-scroll-view": "REPLACE_WITH_VERSION",
    "react-redux": "REPLACE_WITH_VERSION",
    "redux": "REPLACE_WITH_VERSION",
    "redux-persist": "REPLACE_WITH_VERSION",
    "redux-saga": "REPLACE_WITH_VERSION",
    "reduxsauce": "REPLACE_WITH_VERSION",
    "seamless-immutable": "REPLACE_WITH_VERSION",
    <%_ if (context.websocket) { _%>
    "webstomp-client": "REPLACE_WITH_VERSION",
    "sockjs-client": "REPLACE_WITH_VERSION",
    "net": "REPLACE_WITH_VERSION",
    <%_ } _%>
    "@react-native-community/masked-view": "REPLACE_WITH_VERSION",
    "@react-navigation/drawer": "REPLACE_WITH_VERSION",
    "@react-navigation/stack": "REPLACE_WITH_VERSION",
    "@react-navigation/native": "REPLACE_WITH_VERSION",
    "@react-navigation/devtools":"REPLACE_WITH_VERSION",
    "react-native-gesture-handler": "REPLACE_WITH_VERSION",
    "react-native-reanimated": "REPLACE_WITH_VERSION",
    "react-native-safe-area-context": "REPLACE_WITH_VERSION",
    "react-native-screens": "REPLACE_WITH_VERSION",
    "react-native-sectioned-multi-select": "REPLACE_WITH_VERSION",
    "react-native-web": "REPLACE_WITH_VERSION",
    "@expo/vector-icons": "REPLACE_WITH_VERSION",
    "expo-constants": "REPLACE_WITH_VERSION",
    "expo-linking": "REPLACE_WITH_VERSION",
    "expo-splash-screen": "REPLACE_WITH_VERSION",
    "formik": "REPLACE_WITH_VERSION",
    "@react-native-community/datetimepicker": "REPLACE_WITH_VERSION",
    "@react-native-picker/picker": "REPLACE_WITH_VERSION",
    "react-datepicker": "REPLACE_WITH_VERSION",
    "react-native-modal-datetime-picker": "REPLACE_WITH_VERSION",
    "react-native-picker-select": "REPLACE_WITH_VERSION",
    <%_ if (context.authenticationType === 'oauth2') { _%>
    "expo-auth-session": "REPLACE_WITH_VERSION",
    "expo-web-browser": "REPLACE_WITH_VERSION",
    "expo-random": "REPLACE_WITH_VERSION",
    <%_ } _%>
    "yup": "REPLACE_WITH_VERSION"
  },
  "devDependencies": {
    "@storybook/addons": "REPLACE_WITH_VERSION",
    "@storybook/react-native": "REPLACE_WITH_VERSION",
    "@storybook/theming": "REPLACE_WITH_VERSION",
    "babel-preset-env": "REPLACE_WITH_VERSION",
    "buffer": "REPLACE_WITH_VERSION",
    "enzyme": "REPLACE_WITH_VERSION",
    "enzyme-adapter-react-16": "REPLACE_WITH_VERSION",
    "@react-native-community/eslint-config": "REPLACE_WITH_VERSION",
    "babel-jest": "REPLACE_WITH_VERSION",
    "jest": "REPLACE_WITH_VERSION",
    "expect": "REPLACE_WITH_VERSION",
    "jest-expo": "REPLACE_WITH_VERSION",
    "eslint": "REPLACE_WITH_VERSION",
    "eslint-config-prettier": "REPLACE_WITH_VERSION",
    "eslint-plugin-prettier": "REPLACE_WITH_VERSION",
    "generator-jhipster-react-native": "<%= context.packageVersion %>",
    <%_ if (!context.skipCommitHook) { _%>
    "lint-staged": "REPLACE_WITH_VERSION",
    <%_ } _%>
    <%_ if (context.detox) { _%>
    "detox": "REPLACE_WITH_VERSION",
    "detox-expo-helpers": "REPLACE_WITH_VERSION",
    "expo-detox-hook": "REPLACE_WITH_VERSION",
    "jest-circus": "REPLACE_WITH_VERSION",
    <%_ } _%>
    "@expo/webpack-config": "REPLACE_WITH_VERSION",
    "redux-devtools-extension": "REPLACE_WITH_VERSION",
    "mockery": "REPLACE_WITH_VERSION",
    "prettier": "REPLACE_WITH_VERSION",
    "patch-package": "REPLACE_WITH_VERSION",
    "rimraf": "REPLACE_WITH_VERSION"
  },
  <%_ if (!context.skipCommitHook) { _%>
  "lint-staged": {
    "{,app/**/,test/**/}*.{js,json,yml,md,ts,tsx}": [
      "prettier --write",
      "eslint --fix --ext .js"
    ]
  },
  <%_ } _%>
  "jest": {
    "testMatch": [
      "**/test/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns":[
      "/node_modules/",
      "/e2e/",
      "test/setup.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./test/setup"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native-community|react-native|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|@storybook))"
    ],
    "coveragePathIgnorePatterns": [
      "app/shared/services/api.js",
      "app/shared/themes/",
      "/storybook/",
      "/*\\.story\\.js"
    ],
    "preset": "jest-expo"
  },
  "engines": {
    "node": ">=12.x",
    "npm": ">=6.x"
  }
}
