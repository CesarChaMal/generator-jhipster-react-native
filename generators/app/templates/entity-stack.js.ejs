import * as React from 'react';
import { createStackNavigator } from '@react-navigation/stack';

// import screens
import EntitiesScreen from './app/modules/entities/entities-screen';
// jhipster-react-native-navigation-import-needle

const screens = [
  {
    name: 'Entities',
    route: '',
    component: EntitiesScreen,
    options: { title: 'Entities' },
  },
  // jhipster-react-native-TODO-FIX-ME
]

const config = { screens: { }};
screens.forEach(screen => {
  config.screens[screen.name] = screen.route
});

const EntityStack = createStackNavigator();

export default function EntityStackScreen() {
  return (
    <EntityStack.Navigator>
      {(
        screens.map((screen, index) => {
          return <EntityStack.Screen
            name={screen.name}
            component={screen.component}
            key={index}
            options={screen.options || {}}
          />;
        })
      )}

    </EntityStack.Navigator>
  );
}
